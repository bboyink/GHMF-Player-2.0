{
  "name": "ghmf-playback-electron",
  "version": "2.0.0",
  "description": "GHMF Playback 2.0 - Electron + React + TypeScript",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "tsc -p electron/tsconfig.json && electron .",
    "build": "tsc -p electron/tsconfig.json && vite build",
    "build:electron": "electron-builder",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "csv-parse": "^5.5.3",
    "fs-extra": "^11.2.0",
    "howler": "^2.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "recharts": "^2.12.0",
    "serialport": "^12.0.0",
    "wavesurfer.js": "^7.7.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/howler": "^2.2.11",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "^28.2.1",
    "electron-builder": "^24.9.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vite-plugin-electron": "^0.28.2"
  },
  "author": "GHMF",
  "license": "ISC"
}
